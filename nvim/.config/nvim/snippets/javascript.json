{
  "Console log": { "prefix": "log", "body": "console.log(${1});$0" },
  "Strict mode": { "prefix": "st", "body": "\"use strict\";" },
  "Arrow function": {
    "prefix": "afn",
    "body": "const ${1:name} = (${2:params}) => ${0};"
  },
  "For of": {
    "prefix": "forof",
    "body": ["for (const ${1:item} of ${2:array}) {", "\t${0}", "}"]
  },
  "Import": { "prefix": "imp", "body": "import ${1:module} from '${2:path}';" },
  "Export": { "prefix": "exp", "body": "export ${1:default} ${2:name};" },
  "Fetch": {
    "prefix": "fetch",
    "body": [
      "fetch('${1:url}')",
      "\t.then(res => res.json())",
      "\t.then(data => {",
      "\t\t${0}",
      "\t})",
      "\t.catch(console.error);"
    ]
  }
}
