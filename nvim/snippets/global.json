{
  "Year": { "prefix": "year", "body": "${CURRENT_YEAR}" },
  "Date": {
    "prefix": "date",
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"
  },
  "DateTime": {
    "prefix": "datetime",
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}"
  },
  "Email": { "prefix": "email", "body": "user@example.com" },
  "Signature": { "prefix": "sig", "body": "-- ${USER}" },
  "Timestamp": {
    "prefix": "ts",
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"
  },
  "Lorem": {
    "prefix": "lorem",
    "body": "Lorem ipsum dolor sit amet, consectetur adipiscing elit."
  }
}
